#
# Generated by phpLDAPadmin (http://phpldapadmin.sourceforge.net) on January 25, 2019 3:56 am
# Version: 1.2.3
\n
version: 1
\n
# Entry 1: cn=xmen,ou=Group,dc=nti310,dc=local
