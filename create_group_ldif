gidnumber: 503
objectclass: posixGroup
objectclass: top
\n
# Entry 2: cn=knd,ou=Group,dc=nti310,dc=local
dn: cn=knd,ou=Group,dc=nti310,dc=local
cn: knd
gidnumber: 504
objectclass: posixGroup
